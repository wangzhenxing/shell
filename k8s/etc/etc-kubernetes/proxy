# kubernetes proxy config

# default config should be adequate

# Add your own!
KUBE_PROXY_ARGS="--bind-address=172.17.77.90 \
                 --cluster-cidr=10.0.0.0/12 \
                 --masquerade-all \
                 --proxy-mode=ipvs \
                 --ipvs-min-sync-period=2s \
                 --ipvs-sync-period=3s \
                 --ipvs-scheduler=rr \
                 --kubeconfig=/etc/kubernetes/kube-proxy.kubeconfig"
